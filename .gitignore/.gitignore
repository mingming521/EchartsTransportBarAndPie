var result = {};
function toPercent(point) {
    var str = Number(point * 100).toFixed(1);
    str += "%";
    return str;
}
//ajax切换饼图，封装echarts
function refreshnode() {
    $.ajax({
        type: "get",
        async: true, //同步执行
        url: "result.json",
        dataType: "json", //返回数据形式为json
        success: function (data) {
            console.log(data.object[0].length);
            result = data;
            var option1 = {};
            //var myname = ['加电故障', '排水故障', '给水故障', '机械故障', '电机故障'];
            var res = [];
            for (var j = 0; j < result.object.length; j++) {

                var c1 = toPercent((12 + (j + 1) * 15)/100);
                var c2 = toPercent(50/100);
                var item = {
                    name:'故障模式统计' + j,
                   type :'pie',
                   radius :'15%',
                    center: [c1, c2],
                    data: (function () {
                        var res1 = [];
                        // var len = result.length;
                        for (var i = 0, len = result.object[j].length; i < len; i++) {
                            res1.push({
                                //通过把result进行遍历循环来获取数据并放入Echarts中
                                value: result.object[j][i],

                                name: option.legend.data[i],
                            });
                        }
                        return res1;

                    })()
   
}
                option.series.push(item);
            };

            //    option.legend.data = data.object[0];
            //option2.xAxis[0].data = data.object[0];
            //    //option2.series[0].data = data.object[1];
            //}
           
            myChart.clear();
            myChart.hideLoading();
            myChart.setOption(option);
            //myChart2.clear();
            //myChart2.hideLoading();
            //myChart2.setOption(option2);// 使用刚指定的配置项和数据显示图表。
        },
        error: function (errorMsg) {
            alert("图表请求数据失败啦!");
        }
    });
}
